<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.barcicki.gorcalculator"
    android:versionCode="7"
    android:versionName="0.9.5" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="16" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" android:name="GorCalculator">
        <activity android:name="com.barcicki.gorcalculator.CalculatorActivity" android:label="@string/title_calculator">
              <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.barcicki.gorcalculator.PlayerListActivity" android:label="@string/title_player_list">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".CalculatorAcitivity" />
        </activity>
        <meta-data android:name="AA_DB_NAME" android:value="GorCalculator2.db" />
		<meta-data android:name="AA_DB_VERSION" android:value="2" />
    </application>

</manifest>